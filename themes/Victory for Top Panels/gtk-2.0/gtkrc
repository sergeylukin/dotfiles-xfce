		function				= HANDLE
		file					= "Images/null.png"
		orientation				= HORIZONTAL
		}

		image
		{
		function				= HANDLE
		file					= "Images/null.png"
		orientation				= VERTICAL
		}

		#Lines

		image
		{
		function				= VLINE
		file					= "Images/null.png"
		}

		image
		{
		function				= HLINE
		file					= "Images/null.png"
		}
	}
}

