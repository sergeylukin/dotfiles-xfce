style "default"
{
	xthickness					= 1				#Left and Right thickness/padding
	ythickness					= 1				#Top and Bottom thickness/padding

	GtkScrolledWindow::scrollbar-spacing		= 0
	GtkScrolledWindow::scrollbars-within-bevel	= 1

	GtkWindow	::resize-grip-height		= 13
	GtkWindow	::resize-grip-width		= 13

	fg[NORMAL]					= @fg_color
	fg[PRELIGHT]					= @fg_color
	fg[SELECTED]					= @selected_fg_color
	fg[ACTIVE]					= @fg_color
	fg[INSENSITIVE]					= shade (0.8, @bg_color)

	bg[NORMAL]					= @bg_color
	bg[PRELIGHT]					= @bg_color
	bg[SELECTED]					= shade (0.9, @selected_bg_color)
	bg[INSENSITIVE]					= @bg_color
	bg[ACTIVE]					= shade (0.9, @bg_color)

	base[NORMAL]					= @base_color
	base[PRELIGHT]					= shade (0.95, @bg_color)
	base[ACTIVE]					= @selected_bg_color
	base[SELECTED]					= @selected_bg_color
	base[INSENSITIVE]				= @bg_color

	text[NORMAL]					= @text_color
	text[PRELIGHT]					= @text_color
	text[ACTIVE]					= @selected_fg_color
	text[SELECTED]    				= @selected_fg_color
	text[INSENSITIVE]				= shade (0.8, @bg_color)

	engine "murrine"
	{
		arrowstyle				= 0
		animation				= TRUE
		contrast				= 0.4					# Higher for more contrast
#		glazestyle				= 3					# 0flat, 1curved, 2concave, 3top curve, 4beryl highlight
		glow_shade				= 1.1
		glowstyle				= 4					# 0top, 1bottom, 2top&bottom, 3horizontal, 4centered glow
		gradient_shades				= {1.0,1.0,1.0,1.0}
		highlight_shade				= 1.0
		lightborderstyle			= 1					# 0 = lightborder on top side, 1 = lightborder on all sides
		listviewheaderstyle			= 1					# 0 = flat, 1 = glassy, 2 = raised
		listviewstyle				= 0					# 0 = nothing, 1 = dotted
		reliefstyle				= 4					# 0 = flat, 1 = inset, 2 = shadow
		rgba					= FALSE					# FALSE = disabled, TRUE = enabled
		roundness				= 3					# 0 = Square. Higher increases roundness
#		focusstyle				= 3
		separatorstyle				= 1
	}
}

